<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:Tsukuru.ViewModels"
                    xmlns:views="clr-namespace:Tsukuru.Views">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Maps/Compiler/ViewTemplates.xaml" />
        <ResourceDictionary Source="/SourcePawn/ViewTemplates.xaml" />
        <ResourceDictionary Source="/Translator/ViewTemplates.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <DataTemplate DataType="{x:Type viewModels:OptionsViewModel}">
        <views:OptionsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:AboutViewModel}">
        <views:AboutView />
    </DataTemplate>
</ResourceDictionary>