                    xmlns:viewModels="clr-namespace:Tsukuru.SourcePawn.ViewModels"
                    xmlns:views="clr-namespace:Tsukuru.SourcePawn.Views">

    <DataTemplate DataType="{x:Type viewModels:SourcePawnCompileViewModel}">
        <views:SourcePawnCompilerView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:SettingsViewModel}">
        <views:SettingsView />
